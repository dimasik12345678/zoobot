def vigladit(message: telebot.types.Message):
    keyboard = types.InlineKeyboardMarkup(row_width=2)
    btn1 = types.InlineKeyboardButton(text="Миленько", callback_data="Миленько")
    btn2 = types.InlineKeyboardButton(text="Устрашающе ", callback_data="Устрашающе")
    btn3 = types.InlineKeyboardButton(text="Нейтрально", callback_data="Нейтрально")
    btn4 = types.InlineKeyboardButton(text="Без разницы", callback_data="Без разницы")
    text = (" Как выглядит животное?")
    keyboard.add(btn1, btn2, btn3, btn4, )
    bot.send_message(message.chat.id, text, reply_markup=keyboard)


