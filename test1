#обработчик юзера для фото и шрифты, подправить сообщения +таймслип

def result(message: telebot.types.Message):
    if user_data == {'group': 'Млекопитающие',
                     'obraz': 'Ночной',
                     'vigladit': 'Миленько',
                     'haract': 'Спокойный'
                     }:
        bot.send_media_group(message.chat.id, [telebot.types.InputMediaPhoto(open('3332.jpg', 'rb')),
                                               telebot.types.InputMediaPhoto(open('photo1.png', 'rb'))])

    if user_data == {'group': 'Млекопитающие',
                     'obraz': 'Ночной',
                     'vigladit': 'Миленько',
                     'haract': 'Игривый'
                     }:
        bot.send_media_group(message.chat.id, [telebot.types.InputMediaPhoto(open('3332.jpg', 'rb')),
                                               telebot.types.InputMediaPhoto(open('photo1.png', 'rb'))])

    if user_data == {'group': 'Млекопитающие',
                     'obraz': 'Ночной',
                     'vigladit': 'Миленько',
                     'haract': 'Дерзкий'
                     }:
        bot.send_media_group(message.chat.id, [telebot.types.InputMediaPhoto(open('3332.jpg', 'rb')),
                                               telebot.types.InputMediaPhoto(open('photo1.png', 'rb'))])
    if user_data == {'group': 'Млекопитающие',
                     'obraz': 'Ночной',
                     'vigladit': 'Миленько',
                     'haract': 'Милый'
                     }:
        bot.send_media_group(message.chat.id, [telebot.types.InputMediaPhoto(open('3332.jpg', 'rb')),
                                               telebot.types.InputMediaPhoto(open('photo1.png', 'rb'))])
    if user_data == {'group': 'Млекопитающие',
                     'obraz': 'Ночной',
                     'vigladit': 'Устрашающе',
                     'haract': 'Спокойный'
                     }:
