def harakter(message: telebot.types.Message):
    keyboard = types.InlineKeyboardMarkup(row_width=2)
    btn1 = types.InlineKeyboardButton(text="Спокойный", callback_data="Спокойный")
    btn2 = types.InlineKeyboardButton(text="Игривый", callback_data="Игривый")
    btn3 = types.InlineKeyboardButton(text="Дерзкий", callback_data="Дерзкий")
    btn4 = types.InlineKeyboardButton(text="Милый", callback_data="Милый")
    text = ("Какой характер у животного?")
    keyboard.add(btn1, btn2, btn3, btn4, )
    bot.send_message(message.chat.id, text, reply_markup=keyboard)



@bot.callback_query_handler(func=lambda call: call.data == 'Спокойный')
def spok(call):
    user_data['haract'] = 'Спокойный'
    message = call.message
    chat_id = message.chat.id
    bot.send_message(chat_id, f'ваш выбор {call.data}')
    bot.register_next_step_handler(call.message, result(message))

