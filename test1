def obraz(message: telebot.types.Message):
    keyboard = types.InlineKeyboardMarkup(row_width=2)
    btn1 = types.InlineKeyboardButton(text="Ночной", callback_data="Ночной")
    btn2 = types.InlineKeyboardButton(text="Дневной", callback_data="Дневной")
    btn3 = types.InlineKeyboardButton(text="Смешанный", callback_data="Смешанный")
    btn4 = types.InlineKeyboardButton(text="Без разницы", callback_data="Без разницы")
    text = ("Какой образ жизни ведет животное?")
    keyboard.add(btn1, btn2, btn3, btn4, )
    bot.send_message(message.chat.id, text, reply_markup=keyboard)



