import telebot
from telebot import types
import time


bot = telebot.TeleBot(token="svoi")
user_data= {
    'group': '',
    'obraz': '',
    'vigladit': '',
    'haract': ''
}


@bot.message_handler(commands=['start', 'go'])
def zoo(message: telebot.types.Message):
    keyboard = types.InlineKeyboardMarkup(row_width=2)
    btn1 = types.InlineKeyboardButton(text="Млекопитающие", callback_data="Млекопитающие")
    btn2 = types.InlineKeyboardButton(text="Птицы", callback_data="Птицы")
    btn3 = types.InlineKeyboardButton(text="Рыбы", callback_data="Рыбы")
    btn4 = types.InlineKeyboardButton(text="Рептилии", callback_data="Рептилии")
    text = ("Вопрос первый: Какая группа животных тебе больше нравится?")
    keyboard.add(btn1, btn2, btn3, btn4, )
    bot.send_message(message.chat.id, text,reply_markup=keyboard)


@bot.callback_query_handler(func=lambda call: call.data == 'Млекопитающие')
def mlecopit(call):
    user_data['group'] = 'Млекопитающие'
    message = call.message
    chat_id = message.chat.id
    bot.send_message(chat_id, f'ваш выбор {call.data}')
    time.sleep(5)
    bot.register_next_step_handler(call.message, obraz(message))




